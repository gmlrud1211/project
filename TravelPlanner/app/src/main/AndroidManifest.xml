<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.db_14.travelplanner">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:name=".customFont"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.NoTitleBar">
        <activity android:name=".IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".ListActivity" />
        <activity android:name=".SigunguActivity" />
        <activity android:name=".SightsActivity" />
        <activity android:name=".SightViewActivity" />
        <activity android:name=".SightMapActivity" />
        <activity android:name=".UserPlanActivity" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".PlanAddActivity" />
        <activity android:name=".BookmarkActivity" />
        <activity android:name=".AccountEditActivity" />
        <activity android:name=".AccountAddActivity" />
        <activity android:name=".PlanViewActivity" />
        <activity android:name=".InviteListActivity"/>
        <activity
            android:name=".ReviewDetailActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".ReviewActivity" />
        <activity android:name=".ReviewAddActivity" />
        <activity android:name=".PlanEditActivity" />
        <activity android:name=".MyReviewActivity" />
        <activity
            android:name=".MainUIActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"/>

        <service
            android:name=".InviteService"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>