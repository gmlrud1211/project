<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/plan_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:gravity="center_horizontal"
        android:text="(입력된 여행제목)"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="13dp" />

    <TextView
        android:id="@+id/toDate"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="시작 일자"
        android:gravity="center_horizontal"
        android:layout_below="@+id/plan_name"
        android:layout_alignLeft="@+id/count_date"
        android:layout_alignStart="@+id/count_date" />

    <TextView
        android:id="@+id/fromDate"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="도착날짜"
        android:gravity="center_horizontal"
        android:layout_below="@+id/plan_name"
        android:layout_alignRight="@+id/people_num"
        android:layout_alignEnd="@+id/people_num" />




    <TextView
        android:id="@+id/people_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(등록된인원수)"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_marginRight="23dp"
        android:layout_marginEnd="23dp"
        android:layout_marginTop="32dp"
        android:layout_below="@+id/plan_name"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/count_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1일차"
        android:layout_below="@+id/people_num"
        android:layout_toLeftOf="@+id/maker3"
        android:layout_toStartOf="@+id/maker3" />

    <ImageView
        android:id="@+id/maker1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/maker"
        android:layout_marginTop="43dp"
        android:layout_below="@+id/maker2"
        android:layout_toLeftOf="@+id/place3"
        android:layout_toStartOf="@+id/place3"
        android:layout_marginRight="42dp"
        android:layout_marginEnd="42dp" />

    <EditText
        android:id="@+id/place1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="추가된 장소1"
        android:layout_alignBottom="@+id/maker3"
        android:layout_alignLeft="@+id/place2"
        android:layout_alignStart="@+id/place2" />

    <ImageView
        android:id="@+id/maker2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/maker"
        android:layout_marginTop="44dp"
        android:layout_below="@+id/maker3"
        android:layout_alignLeft="@+id/maker1"
        android:layout_alignStart="@+id/maker1" />

    <EditText
        android:id="@+id/place2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="추가된 장소2"
        android:layout_marginRight="35dp"
        android:layout_marginEnd="35dp"
        android:layout_alignBottom="@+id/maker2"
        android:layout_alignRight="@+id/people_num"
        android:layout_alignEnd="@+id/people_num" />



    <ImageView
        android:id="@+id/maker3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        app:srcCompat="@drawable/maker"
        android:layout_below="@+id/count_date"
        android:layout_alignLeft="@+id/maker2"
        android:layout_alignStart="@+id/maker2" />


    <EditText
        android:id="@+id/place3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="추가된 장소3"
        android:layout_alignBottom="@+id/maker1"
        android:layout_alignLeft="@+id/place2"
        android:layout_alignStart="@+id/place2" />

    <TextView
        android:id="@+id/count_date2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2일차"
        android:layout_marginTop="36dp"
        android:layout_below="@+id/maker1"
        android:layout_toLeftOf="@+id/maker1"
        android:layout_toStartOf="@+id/maker1" />

    <ImageView
        android:id="@+id/maker4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/maker"
        android:layout_alignBottom="@+id/place4"
        android:layout_toRightOf="@+id/count_date2"
        android:layout_toEndOf="@+id/count_date2" />


    <EditText
        android:id="@+id/place4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="추가된 장소4"
        android:layout_below="@+id/count_date2"
        android:layout_alignLeft="@+id/place3"
        android:layout_alignStart="@+id/place3"
        android:layout_marginTop="24dp" />


</RelativeLayout>